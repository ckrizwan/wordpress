{
  "name": "rt/gemini-wp-backend",
  "description": "PHP backend for React app, Gemini, and WordPress MCP integration.",
  "type": "project",
  "require": {
    "php": ">=8.1",
    "gemini-api-php/client": "^1.0",
    "guzzlehttp/guzzle": "^7.0"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "php-http/discovery": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
